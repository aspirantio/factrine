{
  "name": "aspirant/factrine",
  "description": "An entity factory for Doctrine ORM",
  "type": "symfony-bundle",
  "require": {
    "doctrine/orm": "^2.5",
    "fzaninotto/faker": "^1.6",
    "symfony/property-access": "^3.2",
    "dflydev/dot-access-data": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Aspirant\\Factrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aspirant\\Factrine\\Tests\\": "tests"
    }
  },
